library(DiffBind)
all <- dba.analyze("ALL_mol.csv")
all <- dba.analyze(all)
dba.plotPCA(all, contrast = 1plot(all)
dba.plotMA(all, contrast = 1)
dba.plotMA(all, contrast = 2)
dba.plotMA(all, contrast = 3)
dba.plotMA(all, contrast = 4)
dba.plotMA(all, contrast = 5)
dba.plotMA(all, contrast = 6)

dba.plotPCA(all,DBA_FACTOR,label=DBA_ID)
all.DB <- dba.report(all)
ALL <- dba.normalize(all, method=DBA_ALL_METHODS,normalize=DBA_NORM_NATIVE,background=FALSE)
dba.show(all,bContrasts=TRUE)
dbs <- dba.report(tamoxifen, bDB=TRUE, bGain=TRUE, bLoss=TRUE)
profiles <- dba.plotProfile(all)
dba.plotProfile(profiles)
